   {
    "id": "b87817f0-1be0-40d6-679f-1228d9b23b7a",
    "type": "ComponentsTemplateComponent",
    "typeDesc": "Template Component",
    "parent": "OTHERCOMPONENTS",
    "properties": [
     {
      "name": "name",
      "value": "template",
      "type": "Id"
     },
     {
      "name": "listeners",
      "value": "[]",
      "type": "Listeners"
     },
     {
      "name": "parameters",
      "value": "[]",
      "type": "ValuesArray"
     },
     {
      "name": "dataSource",
      "value": "templateQueryTerritory",
      "type": "Datasource"
     },
     {
      "name": "priority",
      "value": 5,
      "type": "Integer"
     },
     {
      "name": "htmlObject",
      "value": "${h:template}",
      "type": "HtmlObject"
     },
     {
      "name": "executeAtStart",
      "value": "true",
      "type": "Boolean"
     },
     {
      "name": "preExecution",
      "value": "function f(){\n    \n    var sparklineOpts = function(state){\n        if(state.id == \"Sparkline\"){\n            return {\n                type: 'bar',\n                height: 50,\n                barWidth: 8\n            };\n        } else {   \n            return {\n                width: 120,\n                type: 'bullet',\n                height: '12',\n                targetColor: '#007fff',\n                performanceColor: '#cccccc',\n                rangeColors: ['transparent','transparent','transparent']\n            };\n        }\n    };\n   \n    this.setAddInOptions(\"templateType\",\"sparkline\", sparklineOpts);\n    \n    var bubbleOpts = {\n        containerSize: 60,\n        //color: \"#DDD\"\n        color: \"#FFE085\"\n    };\n\n    this.setAddInOptions(\"templateType\",\"bubble\", bubbleOpts);\n    \n    var bulletChartOpts = {\n        \n        chartOpts: {\n            height: 40,\n            bulletSize: 16,    \n            bulletSpacing: 150,\n            bulletMargin: 5, \n            bulletRuleLabel_text: function(value) {\n                return Utils.numberFormat(value, \"#A\");\n            },\n        }\n    };\n    \n    this.setAddInOptions(\"templateType\",\"bulletChart\", bulletChartOpts);\n    \n    var linkOpts = {\n        pattern: /\\[(.*?)\\]/g\n    };\n\n    this.setAddInOptions(\"templateType\",\"hyperlink\", linkOpts);\n\n\n    var cccChartOpts = { \n        type: 'PieChart',\n        chartOpts: {\n            width: 200,\n            height: 200,\n            seriesInRows: false,\n            crossTabMode: false,\n            timeSeries: false,\n            legendVisible: false,\n            valuesLabelStyle: 'inside',\n            valuesOptimizeLegibility: true,\n            valueFormat: function(value) {\n                return Utils.numberFormat(value, \"#A\");\n            },\n            clearSelectionMode: false,\n            selectable: false,\n            clickable: false,\n            //inter