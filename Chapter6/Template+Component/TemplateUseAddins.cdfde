    "typeDesc": "mdx over mondrianJndi",
    "parent": "MDX",
    "properties": [
     {
      "name": "name",
      "value": "templateQueryTerritory",
      "type": "Id"
     },
     {
      "name": "access",
      "value": "public",
      "type": "Access"
